{
    "name": "LowerRockLabs/laravel-livewire-tables-daterangefilter",
    "description": "A DateRange filter for Rappasoft Laravel Livewire Tables using FlatPickr",
    "keywords": [
        "lowerrocklabs",
        "livewire",
        "datatables",
        "tables",
        "laravel"
    ],
    "homepage": "https://github.com/LowerRockLabs/laravel-livewire-tables-daterangefilter",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe",
            "email": "joe@lowerrocklabs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0|^9.0",
        "rappasoft/laravel-livewire-tables": "dev-master"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^6.13|^7.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "LowerRockLabs\\LaravelLivewireTablesDateRangeFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LowerRockLabs\\LaravelLivewireTablesDateRangeFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LowerRockLabs\\LaravelLivewireTablesDateRangeFilter\\LaravelLivewireTablesDateRangeFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
